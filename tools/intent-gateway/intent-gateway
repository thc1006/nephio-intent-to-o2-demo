#!/usr/bin/env python3
"""
Intent Gateway CLI entry point script.

This script provides the main executable entry point for the intent-gateway CLI tool.
"""

import sys
import os
from pathlib import Path

# Add the package directory to Python path
package_dir = Path(__file__).parent / "intent_gateway"
sys.path.insert(0, str(package_dir.parent))

# Import and run the CLI
from intent_gateway.cli import main

if __name__ == "__main__":
    main()